<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".custRegister">


    <Button
        android:id="@+id/okbtn"
        android:layout_width="162dp"
        android:layout_height="54dp"
        android:background="@drawable/lgnbtn"
        android:fontFamily="@font/montserrat"
        android:onClick="onClickRegBtn"
        android:text="Register"

        android:textColor="@android:color/background_light"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.96" />

    <EditText
        android:id="@+id/reg_cust_usrnm"
        android:layout_width="300dp"
        android:layout_height="43dp"
        android:background="@drawable/ic_textfield"
        android:drawableLeft="@drawable/ic_user"
        android:ems="10"
        android:fontFamily="@font/montserrat"
        android:hint="  Username"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.473" />

    <EditText
        android:id="@+id/reg_cust_phone"
        android:layout_width="300dp"
        android:layout_height="43dp"
        android:background="@drawable/ic_textfield"
        android:drawableLeft="@drawable/ic_phone"
        android:ems="10"
        android:fontFamily="@font/montserrat"
        android:hint="  Mobile Number"
        android:inputType="number"
        android:paddingLeft="10dp"
        android:maxLength="10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.677" />

    <EditText
        android:id="@+id/reg_cust_password"
        android:layout_width="300dp"
        android:layout_height="43dp"
        android:background="@drawable/ic_textfield"
        android:drawableLeft="@drawable/ic_password"
        android:ems="10"
        android:fontFamily="@font/montserrat"
        android:hint=" Password"
        android:inputType="textPassword"
        android:paddingLeft="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.784" />

    <EditText
        android:id="@+id/reg_cust_mail"
        android:layout_width="300dp"
        android:layout_height="43dp"
        android:background="@drawable/ic_textfield"
        android:drawableLeft="@drawable/ic_mail"
        android:ems="10"
        android:fontFamily="@font/montserrat"
        android:hint="  Mail"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.576" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="426dp"
        android:layout_height="297dp"
        android:layout_alignParentTop="false"

        android:background="@drawable/rr"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/iv_back3"
        android:layout_width="27dp"
        android:layout_height="19dp"
        android:background="@drawable/backbtn"
        android:clickable="true"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.07"
        tools:srcCompat="@drawable/backbtn"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="208dp"
        android:layout_height="59dp"
        android:fontFamily="@font/montserrat_bold"
        android:text="Registration "
        android:textColor="@color/white"
        android:textSize="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.145"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.148" />

</androidx.constraintlayout.widget.ConstraintLayout>